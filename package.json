{
  "name": "mc-models",
  "version": "1.0.0",
  "description": "json schemas for validations and generated ts models",
  "scripts": {
    "test": "jest",
    "prepare": "npm run generate",
    "clean": "rimraf generatedTypes",
    "generate": "node generateTypes.js",
    "dockerLocal": "docker build -t mc-models --build-arg host=localhost . && docker run -d -p 80:80 --name mc-models mc-models",
    "dockerRemove": "docker rm -f mc-models"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "json-schema-to-typescript": "^9.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "commitlint": "^9.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5"
  },
  "husky": {
   "hooks": {
     "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
   }
 },
 "config": {
   "commitizen": {
     "path": "./node_modules/cz-conventional-changelog"
   }
 },
  "private": "true",
  "workspaces": [
    "mc-model-types",
    "mc-schema-validator",
    "nest-schema-validator"
  ]
}
