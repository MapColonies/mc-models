   "license": "ISC",
   "devDependencies": {
      "json-schema-to-typescript": "^9.1.0",
      "rimraf": "^3.0.2",
      "typescript": "^3.9.7"
   },
   "private":"true",
   "workspaces":["mc-model-types","mc-schema-validator","mc-nest-schema-validator"]
}
